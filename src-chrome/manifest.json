{
    "name": "NFT2BB",
    "description": "Replaces all occurrences of NFT with Beanie Baby",
    "version": "1.0.1",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "icons": {
        "16": "/images/nft-2-bb-icon-16.png",
        "32": "/images/nft-2-bb-icon-32.png",
        "48": "/images/nft-2-bb-icon-48.png",
        "128": "/images/nft-2-bb-icon-128.png"
    }
}